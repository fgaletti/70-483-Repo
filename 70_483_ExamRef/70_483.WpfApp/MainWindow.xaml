        <Button x:Name="bntStartAverage" Content="Start Average" HorizontalAlignment="Left" Margin="85,154,0,0" VerticalAlignment="Top" Width="133" Height="53" Grid.Column="1" Click="BntStartAverage_Click"/>
        <TextBox x:Name="txtRandom" HorizontalAlignment="Left" Height="23" Margin="85,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.604,2.092" Grid.Column="1"/>
