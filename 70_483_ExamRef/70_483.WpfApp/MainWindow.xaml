        <TextBox x:Name="txtUrl" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="23" Margin="310,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="212"/>
        <Label x:Name="lblUrlResult" Grid.ColumnSpan="2" Content="lblUrlResult" HorizontalAlignment="Left" Height="100" Margin="327,220,0,0" VerticalAlignment="Top" Width="241"/>
        <Button x:Name="btnListUrls" Grid.ColumnSpan="2" Content="btnListUrls" HorizontalAlignment="Left" Margin="581,154,0,0" VerticalAlignment="Top" Width="144" Height="44" Click="BtnListUrls_Click"/>
        <TextBlock x:Name="txtUrls" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="85" Margin="581,29,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="246"/>
        <Label x:Name="lblResultUrls" Grid.ColumnSpan="2" Content="Label" HorizontalAlignment="Left" Margin="581,220,0,0" VerticalAlignment="Top" Height="658" Width="216"/>
    </Grid>
</Window>
